execute pathogen#infect()
filetype plugin indent on
syntax on 
set background=dark
colorscheme solarized
set number
set paste
map <up> <nop>
map <down> <nop>
map <left> <nop>
map <right> <nop>
imap <up> <nop>
imap <down> <nop>
imap <left> <nop>
imap <right> <nop>
set tabstop=2 softtabstop=0 expandtab shiftwidth=2 smarttab

let g:LatexBox_split_type="new"
